<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cse110.teamproject">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TeamProject">
        <activity
            android:name=".SettingsActivity"
            android:exported="false"
            android:label="@string/settings_title" />
        <activity
            android:name=".PlanActivity"
            android:exported="false"
            android:label="@string/plan"/>
        <activity
            android:name=".ExhibitsDirectionsActivity"
            android:exported="false"
            android:label="@string/directions_title"/>
        <activity
            android:name=".SearchResultsActivity"
            android:exported="false"
            android:label="@string/search_results_title"/>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/search_title" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>